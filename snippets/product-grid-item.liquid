    {% comment %} --> Canonicals Update for SEO {% endcomment %}
    {% comment %} <div href="{{ product.url | within: collection }}" class="grid-item__link">
      <a href="{{ product.url | within: collection }}"  class="grid-product__image-wrap"> {% endcomment %}
      <div href="{{ product.url }}" class="grid-item__link">
