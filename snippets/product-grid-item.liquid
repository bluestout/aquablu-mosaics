{%- assign sq_ft = false -%}
{%- assign has_specs = false -%}

{%- for tag in product.tags -%}
  {%- if tag contains 'coverage:' and tag contains 'Sqft' or tag contains 'sqft' -%}
    {% assign sq_ft = tag | remove: 'coverage:' | remove: ' sqft' | remove: ' Sqft' | times: 1.00 %}
    {% assign sq_ft_type = 'Sqft' %}

    {% comment %} <p> {{tag | remove: 'coverage:' | remove: ' Sqft' | times: 1.00 }} </p> {% endcomment %}
   

  {%- endif -%}
  {%- if tag contains 'sold by:' -%}
    {%- assign per = tag | remove: 'sold by:' | prepend: 'per '  -%}
  {%- endif -%}
  {%- if product.type == 'Pool Mosaic' %}
    {% assign per = 'each' %}
  {%- endif -%}
  {%- if tag contains ':' %}
    {% assign spec = tag | split: ':' | first %}
    {% assign has_specs = true %}
  {%- endif -%}
{%- endfor -%}

{% assign customerDiscount = false %}
{% assign discountTotal = 0 %}
{% assign productSpecificDiscount = false %}
{% assign hideComparePrice = false %}

{% comment %} Check for product-specific discount tags first {% endcomment %}
{% for product_tag in product.tags %}
  {% if product_tag contains 'aqua_' %}
    {% assign tag_parts = product_tag | split: '_' %}
    {% if tag_parts.size == 3 %}
      {% assign customer_tag_to_match = tag_parts[1] | downcase %}
      {% assign discount_percentage = tag_parts[2] | times: 1 %}
      
      {% comment %} Check if customer has matching tag {% endcomment %}
      {% for customer_tag in customer.tags %}
        {% assign customer_tag_lower = customer_tag | downcase %}
        {% if customer_tag_lower == customer_tag_to_match %}
          {% assign customerDiscount = true %}
          {% assign productSpecificDiscount = true %}
          {% assign discountTotal = discount_percentage | divided_by: 100.0 %}
          {% assign wholesalePriceAmount = current_variant.price | times: discountTotal %}
          {% assign wholesalePriceTotal = current_variant.price | minus: wholesalePriceAmount | split: '.' %}
          {% comment %} Hide compare price if discount is 0 {% endcomment %}
          {% if discount_percentage == 0 %}
            {% assign hideComparePrice = true %}
          {% endif %}
          {% break %}
        {% endif %}
      {% endfor %}
      
      {% if productSpecificDiscount %}
        {% break %}
      {% endif %}
    {% endif %}
  {% endif %}
{% endfor %}

{% comment %} If no product-specific discount found, use default customer tag discounts {% endcomment %}
{% unless productSpecificDiscount %}
  {% if customer.tags contains "wholesale" or customer.tags contains "Wholesale" %}
      {% assign customerDiscount = true %}
      
      {% assign discountTotal = 0.25 %}
      {% assign wholesalePriceAmount = current_variant.price | times: discountTotal %}
      {% assign wholesalePriceTotal = current_variant.price  | minus: wholesalePriceAmount | split: '.'%}
  {% endif %}

  {% if customer.tags contains "vip" or customer.tags contains "VIP" %}
      {% assign customerDiscount = true %}
      
      {% assign discountTotal = 0.20 %}
      {% assign wholesalePriceAmount = current_variant.price | times: discountTotal %}
      {% assign wholesalePriceTotal = current_variant.price  | minus: wholesalePriceAmount | split: '.'%}
  {% endif %}

  {% if customer.tags contains "trade" or customer.tags contains "Trade" %}
      {% assign customerDiscount = true %}
      
      {% assign discountTotal = 0.15 %}
      {% assign wholesalePriceAmount = current_variant.price | times: discountTotal %}
      {% assign wholesalePriceTotal = current_variant.price  | minus: wholesalePriceAmount | split: '.'%}
  {% endif %}
{% endunless %}

{% comment %} Fix Discounted Pricing not showing on Collection Pages {% endcomment %}
{% comment %} Fix the on_sale logic by the price_max {% endcomment %}
{%- liquid
  assign on_sale = false
  if sq_ft
    if product.compare_at_price_max > product.price_max
      assign on_sale = true
    endif
  else
    if product.compare_at_price > product.price
      assign on_sale = true
    endif
  endif

  assign product_tags = product.tags | join: ','
  assign has_custom_label = false
  assign custom_labels = ''
  if product.metafields.theme.label and product.metafields.theme.label != blank
    assign has_custom_label = true
    assign custom_labels = product.metafields.theme.label.value
  elsif product_tags contains '_label_'
    for tag in product.tags
      if tag contains '_label_'
        assign tag_starts_with = tag | slice: 0
        if tag_starts_with == '_'
          assign has_custom_label = true
          assign custom_label = tag | replace: '_label_', ''
          assign custom_labels = custom_labels | append: custom_label | append: ','
        endif
      endif
    endfor
  endif
-%}

{% comment %} --> Add Wish List {% endcomment %}
{%- assign has_sample = false -%}
{%- assign current_variant_price = false -%}
{%- assign sample_variant = false -%}
{%- assign sample_current_variant = false -%}
{%- unless product.has_only_default_variant -%}
    {% for option in product.variants %}
        {% if option.title == "Sample" %}
        {% comment %} <p>{{option.id}} -- {{option.title}} -- {{option.price}} </p> {% endcomment %}
        {%- assign has_sample = true -%}
        {%- assign current_variant_price = option.price -%}
        {%- assign sample_variant = option -%}
        {%- assign sample_current_variant = option -%}

        {% comment %} <p> {{option}}  </p> {% endcomment %}

    {% endif %}
    {% endfor %}
{%- endunless -%}
