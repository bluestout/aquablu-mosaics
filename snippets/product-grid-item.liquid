              <div class="grid-product__price collection-pricing {{wholesalePriceTotal[0]}} ">
                {% if customerDiscount %}
                
                  {% comment %} B2BCustomer Discount original product price {% endcomment %}
                  {% assign per_sq_ft = wholesalePriceTotal[0] | divided_by: sq_ft %}
                  <span {% if current_variant.title contains 'Sample' %}style="display:none;"{% endif %}>
                    <span data-sq-ft-price="{{per_sq_ft | round}}" class="wholesale-sqft-one collection-pricing-customer-discount{% unless hideComparePrice %} grid-product__price--savings{% endunless %}">
                    {%- render 'price' with per_sq_ft -%}
                    per sq. ft.
                    </span> 
                  </span>
                  {%- if on_sale -%}
                    {% comment %} Fix Discounted Pricing not showing on Collection Pages {% endcomment %}
                    {% comment %} product.compare_at_price --> product.compare_at_price_max {% endcomment %}
                    {% assign per_sq_ft = product.compare_at_price_max | divided_by: sq_ft %}
                    <p>                           
                      <span {% if current_variant.title contains 'Sample' %}style="display:none;"{% endif %}>
                        <span data-sq-ft-price="{{per_sq_ft | round}}" class="wholesale-sqft-one grid-product__price--original">
                        {%- render 'price' with per_sq_ft -%}
                        </span> 
